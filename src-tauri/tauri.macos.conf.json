{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"app": {
		"trayIcon": {
			"id": "main",
			"iconAsTemplate": true,
			"iconPath": "icons/icon-template.png"
		}
	},
	"bundle": {
		"targets": [
			"app",
			"dmg"
		],
		"macOS": {
			"dmg": {
				"appPosition": {
					"x": 180,
					"y": 170
				},
				"applicationFolderPosition": {
					"x": 480,
					"y": 170
				},
				"windowSize": {
					"height": 400,
					"width": 660
				},
				"windowPosition": {
					"x": 200,
					"y": 180
				}
			},
			"frameworks": [],
			"exceptionDomain": "",
			"signingIdentity": null,
			"entitlements": null,
			"minimumSystemVersion": "10.13"
		}
	}
}