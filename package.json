{
  "name": "nvm-desktop",
  "productName": "NVM-Desktop",
  "version": "1.2.0",
  "description": "A desktop client for manage the version of Nodejs.",
  "keywords": [
    "node",
    "electron",
    "nvm-desktop",
    "nvm",
    "version",
    "manager"
  ],
  "homepage": "https://github.com/1111mp/nvm-desktop#readme",
  "bugs": {
    "url": "https://github.com/1111mp/nvm-desktop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1111mp/nvm-desktop.git"
  },
  "license": "MIT",
  "main": "./src/main/main.ts",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:test": "concurrently \"npm run build:main:test\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:main:test": "cross-env TEST=true NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "package": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish never",
    "package:debug": "ts-node ./.erb/scripts/clean.js dist && cross-env DEBUG_PROD=true npm run build && electron-builder build --publish never",
    "package:test": "ts-node ./.erb/scripts/clean.js dist && npm run build:test && electron-builder build --publish never",
    "publish": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish always -mwl",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "wdio run ./wdio.conf.ts"
  },
  "browserslist": [],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@wdio/spec-reporter": "^8.14.0",
    "antd": "^5.7.3",
    "compressing": "^1.9.0",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^6.1.1",
    "fs-extra": "^11.1.1",
    "got-cjs": "^12.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-router-dom": "^6.14.2",
    "react-window": "^1.8.9",
    "tar-fs": "^3.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@electron/notarize": "^2.1.0",
    "@electron/rebuild": "^3.2.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^8.0.1",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/webdriverio": "^3.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.196",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-window": "^1.8.5",
    "@types/tar-fs": "^2.0.1",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/uuid": "^9.0.2",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@wdio/cli": "^8.14.4",
    "@wdio/globals": "^8.14.3",
    "@wdio/local-runner": "^8.14.3",
    "@wdio/mocha-framework": "^8.14.0",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "chromedriver": "114",
    "concurrently": "^8.2.0",
    "core-js": "^3.31.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "detect-port": "^1.5.1",
    "electron": "^25.3.2",
    "electron-builder": "^24.6.3",
    "electron-devtools-installer": "^3.2.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.0.6",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "expect": "^29.6.2",
    "expect-webdriverio": "^4.2.7",
    "file-loader": "^6.2.0",
    "global-jsdom": "^9.1.0",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "is-plain-obj": "3.0.0",
    "jsdom": "^22.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.0",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.1.6",
    "url-loader": "^4.1.1",
    "wdio-electron-service": "^4.3.0",
    "webdriverio": "^8.14.3",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "build": {
    "productName": "nvm-desktop",
    "appId": "org.erb.nvmd",
    "asar": true,
    "artifactName": "${productName}_Setup_${os}_${arch}_${version}.${ext}",
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": ".erb/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "requestedExecutionLevel": "requireAdministrator"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**",
      "./_locales/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "1111mp",
      "repo": "nvm-desktop"
    }
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  }
}
