{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": [
		"main"
	],
	"permissions": [
		"path:default",
		"event:default",
		"window:default",
		"window:allow-close",
		"window:allow-minimize",
		"window:allow-unminimize",
		"window:allow-show",
		"window:allow-set-focus",
		"app:default",
		"image:default",
		"resources:default",
		"menu:default",
		"tray:default",
		"shell:allow-open"
	]
}