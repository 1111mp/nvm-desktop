{
	"productName": "nvm-desktop",
	"version": "0.0.0",
	"identifier": "com.tauri.dev",
	"build": {
		"beforeDevCommand": "pnpm ui:dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "pnpm ui:build",
		"frontendDist": "../dist"
	},
	"app": {
		"macOSPrivateApi": true,
		"windows": [],
		"security": {
			"csp": null,
			"assetProtocol": {
				"enable": true,
				"scope": [
					"$APPDATA/**",
					"$RESOURCE/../**",
					"**"
				]
			}
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}